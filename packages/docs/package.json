{
  "name": "cheetah-grid-docs",
  "private": true,
  "version": "0.14.0",
  "scripts": {
    "watch": "vuepress dev . --debug",
    "build": "npm-run-all build:*",
    "build:vuedoc": "node ./scripts/vuedoc/build.js",
    "build:vuepress": "cross-env NODE_ENV=\"production\" vuepress build .",
    "_build:jsdoc": "jsdoc ./../cheetah-grid/src/js -c jsdoc-conf.js",
    "build:tsdoc": "typedoc --tsconfig ../cheetah-grid/tsconfig.json --options typedoc.js ../cheetah-grid/src/js"
  },
  "devDependencies": {
    "@vuedoc/md": "^1.6.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-vue-preset": "^0.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "fs-readdir-recursive": "^1.1.0",
    "jsdoc": "^3.6.3",
    "npm-run-all": "^4.1.5",
    "typedoc": "^0.15.5",
    "vuepress": "^1.2.0",
    "webpack-dev-middleware": "3.6.0"
  }
}
