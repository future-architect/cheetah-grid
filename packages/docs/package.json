{
  "name": "cheetah-grid-docs",
  "private": true,
  "version": "0.6.0",
  "scripts": {
    "test": "exit 0",
    "lint": "npm run eslint",
    "build": "npm run build:documents && npm run jsdoc && npm run vuedoc && npm run winproc",
    "jsdoc": "jsdoc ./../cheetah-grid/src/js -c jsdoc-conf.js",
    "vuedoc": "node ./scripts/vuedoc/build.js",
    "build:documents": "node ./scripts/build.js",
    "watch": "node ./scripts/build.js --watch",
    "build:dev": "npm run build:documents:dev && npm run vuedoc:dev",
    "build:documents:dev": "node ./scripts/build.js --dev",
    "vuedoc:dev": "node ./scripts/vuedoc/build.js --dev",
    "eslint": "eslint . --ext .js,.html,.hbs",
    "eslint:fix": "eslint . --fix",
    "winproc": "node ./scripts/winProc.js"
  },
  "devDependencies": {
    "@vuedoc/md": "^1.2.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.3.0",
    "eslint-plugin-html": "^4.0.5",
    "handlebars": "^4.0.10",
    "highlight.js": "^9.12.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.10.0",
    "jstransformer-handlebars": "^1.1.0",
    "marked": "^0.3.19",
    "metalsmith": "^2.3.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-in-place": "^3.0.1",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-static": "0.0.5",
    "metalsmith-watch": "^1.0.3",
    "vue-template-compiler": "^2.5.16"
  }
}
