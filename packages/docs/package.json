{
  "name": "cheetah-grid-docs",
  "private": true,
  "version": "1.16.0",
  "scripts": {
    "watch": "vitepress dev . --open",
    "build": "npm-run-all build:*",
    "build:vuedoc": "node ./scripts/vuedoc/build.js",
    "build:vitepress": "vitepress build .",
    "_build:jsdoc": "jsdoc ./../cheetah-grid/src/js -c jsdoc-conf.js",
    "build:tsdoc": "typedoc --options typedoc.js --tsconfig ../cheetah-grid/tsconfig.json"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.3",
    "eslint": "^8.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-vue-preset": "^0.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsdoc": "^3.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typedoc": "^0.26.6",
    "typescript": "^4.9.0",
    "vitepress": "^1.5.0"
  }
}
